{
    "paths": {
        "sourcePath": "C:\\Users\\UserName\\Downloads",
        "destinationPath": "C:\\Users\\UserName\\Downloads\\Organized"
    },
    "settings": {
        "maxSizeInMb": 5000
    },
    "fileRules": [
        {
            "patterns": [
                ".*Gemini.*"
            ],
            "lifecycle": {
                "enabled": true,
                "action": "trash",
                "daysToKeep": 2
            },
            "destinationFolder": "Gemini",
            "matchBy": "regex"
        },
        {
            "patterns": [
                ".*S\\d{2}E\\d{2}.*"
            ],
            "lifecycle": {
                "enabled": false,
                "action": "delete",
                "daysToKeep": 30
            },
            "destinationFolder": "TV\\Series",
            "matchBy": "regex"
        },
        {
            "patterns": [
                ".*(1080p|...).*\\d{4}.*",
                ".*\\d{4}.*(1080p|...).*"
            ],
            "lifecycle": {
                "enabled": false,
                "action": "delete",
                "daysToKeep": 30
            },
            "destinationFolder": "TV\\Movies",
            "matchBy": "regex"
        },
        {
            "patterns": [
                ".doc",
                ".docx",
                ".dot"
            ],
            "lifecycle": {
                "enabled": true,
                "action": "trash",
                "daysToKeep": 2
            },
            "destinationFolder": "Word",
            "matchBy": "extension"
        },
        {
            "patterns": [
                ".ppt",
                ".pptx",
                ".pps",
                ".pot"
            ],
            "lifecycle": {
                "enabled": true,
                "action": "trash",
                "daysToKeep": 2
            },
            "destinationFolder": "PowerPoint",
            "matchBy": "extension"
        },
        {
            "patterns": [
                ".xls",
                ".xlsx",
                ".xlsm",
                ".xlt"
            ],
            "lifecycle": {
                "enabled": true,
                "action": "trash",
                "daysToKeep": 2
            },
            "destinationFolder": "Excel",
            "matchBy": "extension"
        },
        {
            "patterns": [
                ".avi",
                ".divx",
                ".mov",
                ".mp4",
                ".mpg",
                ".mkv",
                ".wmv",
                ".wpl"
            ],
            "lifecycle": {
                "enabled": true,
                "action": "trash",
                "daysToKeep": 2
            },
            "destinationFolder": "Video",
            "matchBy": "extension"
        },
        {
            "patterns": [
                ".JPEG",
                ".JPG",
                ".jpg",
                ".jpeg",
                ".png",
                ".bmp",
                ".ico",
                ".svg",
                ".webp",
                ".gif",
                ".psd",
                ".heic",
                ".nef",
                ".crw",
                ".ai",
                ".id"
            ],
            "lifecycle": {
                "enabled": true,
                "action": "trash",
                "daysToKeep": 2
            },
            "destinationFolder": "Image",
            "matchBy": "extension"
        },
        {
            "patterns": [
                ".mp3",
                ".mid",
                ".wav",
                ".wma",
                ".cda",
                ".ogm",
                ".acc",
                ".ac3",
                ".flac",
                ".aym"
            ],
            "lifecycle": {
                "enabled": true,
                "action": "trash",
                "daysToKeep": 2
            },
            "destinationFolder": "Audio",
            "matchBy": "extension"
        },
        {
            "patterns": [
                ".zip",
                ".rar",
                ".rar5",
                ".7z",
                ".ace",
                ".gz",
                ".tar.bz2"
            ],
            "lifecycle": {
                "enabled": true,
                "action": "trash",
                "daysToKeep": 2
            },
            "destinationFolder": "Compressed",
            "matchBy": "extension"
        },
        {
            "patterns": [
                ".pdf"
            ],
            "lifecycle": {
                "enabled": true,
                "action": "trash",
                "daysToKeep": 2
            },
            "destinationFolder": "PDF",
            "matchBy": "extension"
        }
    ],
    "defaultFolder": "Other",
    "folderRules": [
        {
            "patterns": [
                "^Organized$"
            ],
            "lifecycle": null,
            "destinationFolder": null,
            "ruleName": "Organized",
            "matchBy": "regex",
            "action": "ignore",
            "deleteEmptyAfterProcessing": false
        },
        {
            "patterns": [
                "^GeminiPrompts$"
            ],
            "lifecycle": {
                "enabled": false,
                "action": "delete",
                "daysToKeep": 2
            },
            "destinationFolder": "Prompts\\Gemini",
            "ruleName": "GeminiPrompts",
            "matchBy": "regex",
            "action": "process_contents",
            "deleteEmptyAfterProcessing": false
        },
        {
            "patterns": [
                "^Ekasoft$"
            ],
            "lifecycle": null,
            "destinationFolder": null,
            "ruleName": "Ekasoft",
            "matchBy": "regex",
            "action": "ignore",
            "deleteEmptyAfterProcessing": false
        },
        {
            "patterns": [
                ".*S\\d{2}E\\d{2}.*"
            ],
            "lifecycle": {
                "enabled": false,
                "action": "trash",
                "daysToKeep": 30
            },
            "destinationFolder": "TV\\Series",
            "ruleName": "MoveSeries",
            "matchBy": "regex",
            "action": "process_contents",
            "deleteEmptyAfterProcessing": true
        },
        {
            "patterns": [
                ".*(1080p|...).*\\d{4}.*",
                ".*\\d{4}.*(1080p|...).*"
            ],
            "lifecycle": {
                "enabled": false,
                "action": "trash",
                "daysToKeep": 30
            },
            "destinationFolder": "TV\\Movies",
            "ruleName": "MoveMovies",
            "matchBy": "regex",
            "action": "process_contents",
            "deleteEmptyAfterProcessing": true
        },
        {
            "patterns": [
                ".*"
            ],
            "lifecycle": null,
            "destinationFolder": "Other",
            "ruleName": "Default",
            "matchBy": "regex",
            "action": "process_contents",
            "deleteEmptyAfterProcessing": true
        }
    ],
    "orderedFiles": [
    ]
}