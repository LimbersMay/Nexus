{
  "version": "4.0.0",
  "zones": [
    {
      "zoneName": "Screenshots",
      "paths": {
        "sourcePath": "C:\\Users\\UserName\\Pictures\\Screenshots",
        "destinationPath": "C:\\Users\\UserName\\Downloads\\Organized\\Screenshots"
      },
      "settings": {
        "maxSizeInMb": 5000
      },
      "rules": [
        {
          "ruleName": "Default",
          "patterns": [
            ".*"
          ],
          "lifecycle": {
            "enabled": true,
            "action": "trash",
            "daysToKeep": 7
          },
          "destinationFolder": "Archive",
          "matchBy": "regex",
          "handlingStrategy": "move"
        }
      ],
      "orderedFiles": []
    },
    {
      "zoneName": "ClipboardInbox",
      "paths": {
        "sourcePath": "C:\\Users\\UserName\\Documents\\PersonalProjects\\ClipboardImageAutomaticSaver\\Clipboard_Inbox",
        "destinationPath": "C:\\Users\\UserName\\Downloads\\Organized\\ClipboardInbox"
      },
      "settings": {
        "maxSizeInMb": 5000
      },
      "rules": [
        {
          "ruleName": "SocialMedia",
          "patterns": [
            "(?i).*\\[.*Facebook.*\\].*",
            "(?i).*\\[.*Twitter.*\\].*",
            "(?i).*\\[.*Reddit.*\\].*"
          ],
          "lifecycle": {
            "enabled": true,
            "action": "trash",
            "daysToKeep": 7
          },
          "destinationFolder": "SocialMedia",
          "matchBy": "regex",
          "handlingStrategy": "move"
        },
        {
          "ruleName": "Default",
          "patterns": [
            ".*"
          ],
          "lifecycle": {
            "enabled": true,
            "action": "trash",
            "daysToKeep": 7
          },
          "destinationFolder": "Other",
          "matchBy": "regex",
          "handlingStrategy": "move"
        }
      ],
      "orderedFiles": []
    },
    {
      "zoneName": "Downloads",
      "paths": {
        "sourcePath": "C:\\Users\\UserName\\Downloads",
        "destinationPath": "C:\\Users\\UserName\\Downloads\\Organized\\Downloads"
      },
      "settings": {
        "maxSizeInMb": 10000
      },
      "rules": [
        {
          "ruleName": "IgnoreTemporal",
          "patterns": [
            "^Temporal$"
          ],
          "lifecycle": null,
          "destinationFolder": null,
          "matchBy": "regex",
          "handlingStrategy": "ignore"
        },
        {
          "ruleName": "GeminiFiles",
          "patterns": [
            ".*Gemini.*"
          ],
          "lifecycle": {
            "enabled": true,
            "action": "trash",
            "daysToKeep": 1
          },
          "destinationFolder": "Gemini",
          "matchBy": "regex",
          "handlingStrategy": "move"
        },
        {
          "ruleName": "TvSeries",
          "patterns": [
            ".*S\\d{2}E\\d{2}.*"
          ],
          "lifecycle": {
            "enabled": false,
            "action": "delete",
            "daysToKeep": 30
          },
          "destinationFolder": "TV\\Series",
          "matchBy": "regex",
          "handlingStrategy": "process_contents",
          "deleteEmptyAfterProcessing": true
        },
        {
          "ruleName": "TvMovies",
          "patterns": [
            "(?i).*[\\. _\\-\\(\\[](19|20)\\d{2}[\\. _\\-\\)\\]].*(1080p|720p|2160p|4k|HDR|DvDrip).*"
          ],
          "lifecycle": {
            "enabled": false,
            "action": "delete",
            "daysToKeep": 30
          },
          "destinationFolder": "TV\\Movies",
          "matchBy": "regex",
          "handlingStrategy": "process_contents",
          "deleteEmptyAfterProcessing": true
        },
        {
          "ruleName": "WordDocs",
          "patterns": [
            ".doc",
            ".docx",
            ".dot"
          ],
          "lifecycle": {
            "enabled": true,
            "action": "trash",
            "daysToKeep": 2
          },
          "destinationFolder": "Word",
          "matchBy": "extension",
          "handlingStrategy": "move"
        },
        {
          "ruleName": "PowerPoint",
          "patterns": [
            ".ppt",
            ".pptx",
            ".pps",
            ".pot"
          ],
          "lifecycle": {
            "enabled": true,
            "action": "trash",
            "daysToKeep": 2
          },
          "destinationFolder": "PowerPoint",
          "matchBy": "extension",
          "handlingStrategy": "move"
        },
        {
          "ruleName": "Excel",
          "patterns": [
            ".xls",
            ".xlsx",
            ".xlsm",
            ".xlt"
          ],
          "lifecycle": {
            "enabled": true,
            "action": "trash",
            "daysToKeep": 2
          },
          "destinationFolder": "Excel",
          "matchBy": "extension",
          "handlingStrategy": "move"
        },
        {
          "ruleName": "VideoFiles",
          "patterns": [
            ".avi",
            ".divx",
            ".mov",
            ".mp4",
            ".mpg",
            ".mkv",
            ".wmv",
            ".wpl"
          ],
          "lifecycle": {
            "enabled": true,
            "action": "trash",
            "daysToKeep": 2
          },
          "destinationFolder": "Video",
          "matchBy": "extension",
          "handlingStrategy": "move"
        },
        {
          "ruleName": "Images",
          "patterns": [
            ".JPEG",
            ".JPG",
            ".jpg",
            ".jpeg",
            ".png",
            ".bmp",
            ".ico",
            ".svg",
            ".webp",
            ".gif",
            ".psd",
            ".heic",
            ".nef",
            ".crw",
            ".ai",
            ".id"
          ],
          "lifecycle": {
            "enabled": true,
            "action": "trash",
            "daysToKeep": 2
          },
          "destinationFolder": "Image",
          "matchBy": "extension",
          "handlingStrategy": "move"
        },
        {
          "ruleName": "Audio",
          "patterns": [
            ".mp3",
            ".mid",
            ".wav",
            ".wma",
            ".cda",
            ".ogm",
            ".acc",
            ".ac3",
            ".flac",
            ".aym"
          ],
          "lifecycle": {
            "enabled": true,
            "action": "trash",
            "daysToKeep": 2
          },
          "destinationFolder": "Audio",
          "matchBy": "extension",
          "handlingStrategy": "move"
        },
        {
          "ruleName": "Compressed",
          "patterns": [
            ".zip",
            ".rar",
            ".rar5",
            ".7z",
            ".ace",
            ".gz",
            ".tar.bz2"
          ],
          "lifecycle": {
            "enabled": true,
            "action": "trash",
            "daysToKeep": 2
          },
          "destinationFolder": "Compressed",
          "matchBy": "extension",
          "handlingStrategy": "move"
        },
        {
          "ruleName": "PDF",
          "patterns": [
            ".pdf"
          ],
          "lifecycle": {
            "enabled": true,
            "action": "trash",
            "daysToKeep": 2
          },
          "destinationFolder": "PDF",
          "matchBy": "extension",
          "handlingStrategy": "move"
        },
        {
          "ruleName": "Default",
          "patterns": [
            ".*"
          ],
          "lifecycle": {
            "enabled": true,
            "action": "trash",
            "daysToKeep": 2
          },
          "destinationFolder": "Other",
          "matchBy": "regex",
          "handlingStrategy": "process_contents",
          "deleteEmptyAfterProcessing": true
        }
      ],
      "orderedFiles": []
    }
  ]
}